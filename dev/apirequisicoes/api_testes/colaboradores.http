# Usuário sem nome requerido.
POST {{apiUrl}}/colaboradores
Content-Type: application/json

{
 "nome": "",
 "email": "jaqson@upf.br",
 "senha": "123"
}
###


# Usuário com email duplicado.
POST {{apiUrl}}/colaboradores
Content-Type: application/json

{
 "nome": "Jaqson",
 "email": "182749@upf.br",
 "senha": "123"
}
###


# Alterar usuário
PUT {{apiUrl}}/colaboradores
Content-Type: application/json

{
 "_id": "5f6cba8798eca62ccce84ba7",
 "nome": "Jaqson alterado",
 "email": "jaqson@teste.com"
}
